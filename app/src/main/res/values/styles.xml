<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="OverFlowToolbar" parent="Widget.AppCompat.Light.PopupMenu.Overflow">
        <item name="overlapAnchor">false</item>
    </style>
    <style name="ToolbarTitleColor" parent="AppTheme">
        <item name="colorPrimary">@color/result_points</item>
    </style>
    <style name="button_change" parent="android:Widget.Button">
        <item name="android:button">@drawable/button_state</item>
    </style>
    <style name="checkbox" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">#ffffff</item>
        <item name="colorControlActivated">#ff0000</item>
    </style>
    <style name="taskid_dialog" parent="AlertDialog.AppCompat">
        <item name="android:windowNoTitle">true</item>
    </style>

</resources>
